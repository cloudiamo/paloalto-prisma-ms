  <command name="CREATE">
    <operation><![CDATA[{literal}
{
{/literal}
  {if isset($params.pre_shared_key)}
  "authentication": {
    "pre_shared_key": {
      "key": "{$params.pre_shared_key}"
    }
  },
  {/if}

  {if isset($params.local_id) && isset($params.local_id_type)}
  "local_id": {
    "id": "{$params.local_id}",
    "type": "{$params.local_id_type}"
  },
  {/if}

  {if isset($params.object_id)}
  "name": "{$params.object_id}",
  {/if}

  {if isset($params.peer_address_ip)}
  "peer_address": {
    "ip": "{$params.peer_address_ip}"
  },
  {/if}

  {if isset($params.peer_id) && isset($params.peer_id_type)}
  "peer_id": {
    "id": "{$params.peer_id}",
    "type": "{$params.peer_id_type}"
  },
  {/if}

  "protocol": {
    "ikev1": {
      "dpd": {
        "enable": {if $params.protocol_ikev1_dpd_enable == true}true{else}false{/if}
      },
      {if isset($params.ikev1_crypto_profile)}
      "ike_crypto_profile": "{$params.ikev1_crypto_profile}"
      {/if}
    },
    "ikev2": {
      "dpd": {
        "enable": {if $params.protocol_ikev2_dpd_enable == true}true{else}false{/if}
      },
      {if isset($params.ikev2_crypto_profile)}
      "ike_crypto_profile": "{$params.ikev2_crypto_profile}"
      {/if}
    },
    {if isset($params.protocol_version)}
    "version": "{$params.protocol_version}"
    {/if}
  },

  "protocol_common": {
    "fragmentation": {
      "enable": {if $params.protocol_fragmentation == true}true{else}false{/if}
    },
    "nat_traversal": {
      "enable": {if $params.nat_traversal_enable == true}true{else}false{/if}
    },
    "passive_mode": {if $params.protocol_passive_mode == true}true{else}false{/if}
  }
{literal}
}
{/literal}
]]></operation>
    <rest>POST</rest>
