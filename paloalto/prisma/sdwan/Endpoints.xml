  <command name="CREATE">
    <operation><![CDATA[{
  "name": "{$params.name}",
  "description": "{$params.description}",
  "tags": null,
  "type": "non-cg-transit",
  "site_id": null,
  "admin_up": {if $params.admin_up == true}true{else}false{/if},
  "service_link_peers": {
    "ip_addresses": [
    {foreach $params.pop_ip as $ip}
      "{$ip.ip_addr}"{if not $ip@last},{/if}
    {/foreach}
    ],
    "hostnames": null
  },
  "allow_enterprise_traffic": false,
  "liveliness_probe": {
    "icmp_ping": [
    {foreach $params.liveliness_probe as $lp}
      {
        "ip_addresses": [
        {foreach $lp.ip as $li}
          "{$li.ip_addr}"
        {/foreach}
        ],
        "interval": {$lp.interval},
        "failure_count": {$lp.failure_count}
      }{if not $lp@last},{/if}
    {/foreach}
